<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.onlinedonationproject">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/donate_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/donate_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginDonorActivity"></activity>
        <activity android:name=".SignupDonoractivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".DetailsActivity" />
        <activity android:name=".PostlistActivity" />
        <activity android:name=".loginfundactivity" />
        <activity android:name=".signupfundactivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>